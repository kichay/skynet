server:
  verbosity: 1
  interface: 192.168.128.1
  interface: 192.168.123.1
  ip-transparent: yes
  do-ip6: no

  access-control: 10.0.0.0/8 allow
  access-control: 192.168.0.0/16 allow

  cache-max-ttl: 3600
  cache-max-negative-ttl: 60
  cache-min-ttl: 60

  hide-identity: yes
  hide-version: yes
  minimal-responses: yes
  qname-minimisation: yes
  prefetch: yes
  rrset-roundrobin: yes
  use-caps-for-id: yes
  tls-cert-bundle: "/etc/ssl/certs/ca-certificates.crt"

  local-zone: "master.skynet" static
  local-data: "master.skynet A 192.168.128.11"
  local-zone: "core.skynet" static
  local-data: "core.skynet A 192.168.128.11"
  local-zone: "celeron.skynet" static
  local-data: "celeron.skynet A 192.168.128.12"
  local-zone: "ryzen.skynet" static
  local-data: "ryzen.skynet A 192.168.128.13"


forward-zone:
  name: "."
   forward-tls-upstream: yes
   forward-addr: 1.0.0.1@853#one.one.one.one
   forward-addr: 1.1.1.1@853#one.one.one.one
   forward-addr: 8.8.4.4@853#dns.google
   forward-addr: 8.8.8.8@853#dns.google
   forward-addr: 9.9.9.9@853#dns.quad9.net
   forward-addr: 149.112.112.112@853#dns.quad9.net
