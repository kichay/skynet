global_defs {
}

vrrp_instance "gateway" {

    interface skynet
    virtual_router_id 1

    authentication {

        auth_type PASS
        auth_pass skynet

    }

    virtual_ipaddress {

        192.168.128.1 dev skynet
        5.130.78.89/24 dev novotelecom
        192.168.123.1/24 dev home

    }

    virtual_routes {

        0.0.0.0/0 via 5.130.78.1 dev novotelecom metric 75 no_track

    }

}
